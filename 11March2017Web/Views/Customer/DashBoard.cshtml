@model _11March2017Web.Models.tblCustomer

@{
    ViewBag.Title = "DashBoard";
}

<div class="panel container">
    <h2>DashBoard</h2>

    <div class="col-md-3">
        @Html.Partial("_CustomerNavBarStacked")
    </div>

    <div class="col-md-9">
        <div class="panel panel-default well well-sm">
            <div class="panel-body">

                <div class="col-md-12">
                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt class="h3">
                                @Html.DisplayNameFor(model => model.customerName)
                            </dt>

                            <dd class="h4">
                                @Html.DisplayFor(model => model.customerName)
                            </dd>
                        </dl>
                    </div>

                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt class="h3">
                                @Html.DisplayNameFor(model => model.customerEmail)
                            </dt>

                            <dd class="h4">
                                @Html.DisplayFor(model => model.customerEmail)
                            </dd>
                        </dl>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="col-sm-10">
                        <dl class="dl-horizontal">
                            <dt class="h3">
                                @Html.DisplayNameFor(model => model.customerAddress)
                            </dt>

                            <dd class="h4">
                                @Html.DisplayFor(model => model.customerAddress)
                            </dd>
                        </dl>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt class="h3">
                                @Html.DisplayNameFor(model => model.customerPhone)
                            </dt>

                            <dd class="h4">
                                @Html.DisplayFor(model => model.customerPhone)
                            </dd>
                        </dl>
                    </div>
                </div>

                <div class="col-md-12">
                    

                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt class="h3">
                                Gender
                            </dt>

                            <dd class="h4">
                                @Html.DisplayFor(model => model.tblGender.genderName)
                            </dd>
                        </dl>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






